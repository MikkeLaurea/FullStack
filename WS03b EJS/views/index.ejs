<!DOCTYPE html>
<html>
<head>
  <title><%= message %></title>
</head>
<body>
  <%- include('partials/_header') %>

  <h1><%= message %></h1>

  <% if (isLoggedIn) { %>
    <p>Welcome back, user! ðŸŽ‰</p>
  <% } else { %>
    <p>Please <a href="#">log in</a> to continue.</p>
  <% } %>

  <h2>Users:</h2>
  <ul>
    <% users.forEach(function(user) { %>
      <li>
        <strong>Name:</strong> <%= user.name %><br>
        <strong>Email:</strong> <%= user.email %><br>
